{
  "name": "gdpr-cash",
  "version": "0.1.0",
  "description": "The GDPR Cash is an ERC20 standard token",
  "main": "truffle.js",
  "directories": {
    "test": "test",
    "contracts": "contracts"
  },
  "dependencies": {
    "truffle-hdwallet-provider": "0.0.3",
    "zeppelin-solidity": "^1.7.0"
  },
  "devDependencies": {
    "solhint": "^1.1.10",
    "solidity-coverage": "^0.4.11",
    "pretty-quick": "1.4.1",
    "husky": "0.14.3"
  },
  "scripts": {
    "test": "truffle test",
    "test:cov": "solidity-coverage",
    "lint:sol": "solhint contracts/**/*.sol",
    "deploy:ropsten": "truffle migrate --network ropsten -f 2",
    "deploy:rinkeby": "truffle migrate --network rinkeby -f 2",
    "precommit": "pretty-quick --staged"
  },
  "repository": "",
  "keywords": [
    "ethereum",
    "ERC20",
    "crowdsale",
    "GDPR",
    "Privacy"
  ],
  "author": "GDPR.CASH Core Team",
  "contributors": [
    "ranapat"
  ],
  "license": "MIT",
  "homepage": "https://gdpr.cash"
}
